<Window x:Class="AutoTest.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Style="{StaticResource modalWindow}"
        Title="About" Height="256" Width="430" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <Image Source="logo.ico" Width="64" Margin="0, 30, 0, 0" />
        <TextBlock Text="C04AutoTest" TextAlignment="Center" FontSize="30" Margin="0, 10, 0, 0" FontWeight="Bold" />
        <Grid Margin="20">
            <TextBlock Text="C0420G1" FontSize="16" RenderTransformOrigin="0.828,0.524" Margin="20,0,135,0" Width="Auto"  />
            <TextBlock x:Name="lblVersion" Text="Version" Margin="249,4,-9,-4" Foreground="Blue" />
        </Grid>
    </StackPanel>
</Window>
